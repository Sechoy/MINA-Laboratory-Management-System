
.expListView {
  width: 100%;
  bottom: 0;
  top: 5rpx;
  /* background-color: red; */
  position: absolute;
}

/* cell */
.item {
  display: flex;
  margin-top: 20px;
  position:relative;
  top:20rpx;
  width: 92%;
  height: 300rpx;
  left: 4%;
  display: inline-block;
  box-shadow:0 4rpx 20rpx 0 rgba(212,217,223,.5);
  border-radius:4px;
}
/* cell 产品图 */
.iconImageView {
  width: 200rpx;
  height: 200rpx;
  top: 50rpx;
  left: 40rpx;
  /* background-color:orange; */
  position: absolute;
}
.iconImage {
  width: 200rpx;
  height: 200rpx;
}
.buttonView {
  width: 170rpx;
  height: 70rpx;
  top: 180rpx;
  left: 260rpx;
  /* background-color:orange; */
  position: absolute;
  font-size: 14px;
}

.expNameView {
  top: 50rpx;
  left: 280rpx;
  right: 130rpx;
  height: 50rpx;
  position: absolute;
}
.expNameText {
  color: rgb(32, 27, 27);
  font-size: 35rpx;
  white-space: nowrap;
    text-overflow:ellipsis;
}

.submitView {
  top: 50rpx;
  right: 10rpx;
  height: 50rpx;
  width: 100rpx;
  position: absolute;
}
.submitText {
  color: rgb(230, 204, 140);
  font-size: 25rpx;
}

/* 具体产品 */
.endView {
  top: 120rpx;
  height: 50rpx;
  left: 280rpx;
  right: 20rpx;
  /* background-color: gray; */
  position: absolute;
}
.endText {
  font-size: 32rpx;
  color: gray;
  display: block;
  /* line-height: 50rpx; */
   overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
   /* -webkit-line-clamp:1;  */
}
